{
  "manifest_version": 2,
  "name": "Key Locker",
  "version": "1.0",

  "description": "This extension prevents your password to be seen by others and prevent the attacks of keyloggers.",
  "icons": { 
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },

  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "32": "images/icon32.png"
    },
    "default_title": "Key Locker",
    "defualt_popup": "popup.html"

  },

  "background": {  
    "persistent": false,  
    "scripts": ["bg.js"]  
  },  

  "author": "Albert Hsieh",
  "homepage_url": "https://github.com/alberthsies/passwd-encrypt-extension",
  "short_name": "KL",
  "update_url": "https://github.com/alberthsies/passwd-encrypt-extension",
  "version_name": "testing version"
}